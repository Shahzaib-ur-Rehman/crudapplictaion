<%for(let i=0;i<users.length;i++){%>
<tr>
    <th scope="row"><%=i+1%></th>
    <td><%=users[i].name%></td>
    <td><%=users[i].Email%></td>
    <td><%=users[i].Gender%></td>
    <td><%=users[i].status%></td>
    <td>
        <a href="/update-user?_id=<%=users[i]._id%>"><i class="far fa-edit user_edit_icon"></i></a>
        <a class="delete" data-id=<%=users[i]._id%>><i class="fas fa-trash-alt user_edit_icon"></i></a>
        
    </td>
  </tr>
  <%}%>